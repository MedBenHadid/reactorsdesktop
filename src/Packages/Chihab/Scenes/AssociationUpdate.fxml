                  <LinearGradient endX="1.0" endY="1.0" startY="0.42748090875057776">
                     <stops>
                        <Stop color="WHITE" />
                        <Stop color="BLACK" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </ripplerFill>
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
