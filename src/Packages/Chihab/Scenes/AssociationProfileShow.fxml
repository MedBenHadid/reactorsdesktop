<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXScrollPane?>
<?import com.jfoenix.controls.JFXSpinner?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fx:id="scrollPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" xmlns="http://javafx.com/javafx/11.0.1">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefWidth="1200.0">
         <children>
             <JFXScrollPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                            AnchorPane.topAnchor="0.0">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                         <ImageView fx:id="imageImageView" fitHeight="250.0" fitWidth="400.0" pickOnBounds="true"
                                    preserveRatio="true"/>
                        <Label fx:id="nameLabel" text="Label" textAlignment="CENTER">
                           <font>
                               <Font name="System Bold Italic" size="42.0"/>
                           </font>
                        </Label>
                         <JFXToggleButton fx:id="statusToggleButton"/>
                         <Button fx:id="afficherPieceButton" mnemonicParsing="false"
                                 text="Afficher piece justificative"/>
                        <VBox alignment="CENTER_LEFT">
                           <children>
                               <Label style="-fx-padding:10;" text="Description :"/>
                               <Label fx:id="descriptionLabel"
                                      text="azeazkllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllllzzzzzzzz  zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzze">
                                 <VBox.margin>
                                     <Insets/>
                                 </VBox.margin>
                                 <font>
                                     <Font size="20.0"/>
                                 </font>
                              </Label>
                               <Label style="-fx-padding:10;" text="Informations de contact :"/>
                              <Label fx:id="numeroLabel" text="Label">
                                 <VBox.margin>
                                     <Insets left="50.0" top="5.0"/>
                                 </VBox.margin>
                              </Label>
                               <JFXSpinner fx:id="spinner"/>
                               <WebView fx:id="mapWebView" prefWidth="-1.0"/>
                               <ScrollPane>
                                   <content>
                                       <AnchorPane minHeight="0.0" minWidth="0.0">
                                           <children>
                                               <VBox fx:id="membersVbox" alignment="CENTER"
                                                     AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                     AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                                           </children>
                                       </AnchorPane>
                                   </content>
                               </ScrollPane>
                           </children>
                           <VBox.margin>
                               <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
                           </VBox.margin>
                        </VBox>
                     </children>
                     <padding>
                         <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
                     </padding>
                  </VBox>
               </children>
            </JFXScrollPane>
         </children>
        </AnchorPane>
    </content>
</ScrollPane>
