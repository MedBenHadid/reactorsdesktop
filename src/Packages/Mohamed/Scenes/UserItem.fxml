<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<HBox xmlns:fx="http://javafx.com/fxml/1" fx:id="h" xmlns="http://javafx.com/javafx/11.0.1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <VBox alignment="CENTER">
        <ImageView fx:id="userImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                   preserveRatio="true"/>
        <Label fx:id="nameLabel" text="fullName"/>
    </VBox>
    <VBox maxHeight="120.0" prefHeight="114.0" prefWidth="82.0">
        <HBox.margin>
            <Insets top="20.0"/>
        </HBox.margin>
        <Label text="Email :">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
        <Label text="Téléphone :">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
        <Label text="Adresse  :">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
    </VBox>
    <VBox maxHeight="120.0" prefWidth="300.0">
        <HBox.margin>
            <Insets top="20.0"/>
        </HBox.margin>
        <Label fx:id="emailLabel">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
        <Label fx:id="telLabel">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
        <Label fx:id="adresseLabel">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
    </VBox>
</HBox>
