<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTimePicker?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="rootPane" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="Packages.Chihab.Controllers.AssociationCreateController">
    <children>
        <ScrollPane hbarPolicy="ALWAYS" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                    AnchorPane.topAnchor="30.0">
            <content>
                <AnchorPane>
                    <children>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <children>
                                <Label text="Création d'une nouvelle association :">
                                    <VBox.margin>
                                        <Insets left="20.0" top="20.0"/>
                                    </VBox.margin>
                                </Label>
                                <Separator>
                                    <VBox.margin>
                                        <Insets left="20.0" right="20.0"/>
                                    </VBox.margin>
                                </Separator>
                                <HBox>
                                    <children>
                                        <Label text="Nom">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <TextField fx:id="nomInput" promptText="Nom de l'association">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                                <HBox>
                                    <children>
                                        <Label text="Description">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <TextArea fx:id="descriptionInput" prefHeight="200.0" prefWidth="200.0"
                                                  promptText="Description de l'association">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </TextArea>
                                    </children>
                                </HBox>
                                <HBox>
                                    <children>
                                        <Label text="Adresse">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <ComboBox fx:id="villeComboBox" prefWidth="150.0" promptText="Ville">
                                            <HBox.margin>
                                                <Insets bottom="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </ComboBox>
                                        <TextField fx:id="rueInput" promptText="Rue">
                                            <HBox.margin>
                                                <Insets bottom="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </TextField>
                                        <TextField fx:id="zipInput" minHeight="4.0" minWidth="4.0" prefWidth="100.0"
                                                   promptText="Code postale">
                                            <HBox.margin>
                                                <Insets bottom="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                                <HBox>
                                    <children>
                                        <Label text="Photo &amp; Piece *">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <Button fx:id="photoButton" mnemonicParsing="false" text="Photo">
                                            <HBox.margin>
                                                <Insets bottom="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Button>
                                        <Button fx:id="pieceButton" mnemonicParsing="false" text="Piece justificative">
                                            <HBox.margin>
                                                <Insets bottom="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Button>
                                    </children>
                                </HBox>
                                <HBox>
                                    <children>
                                        <Label text="Manager">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <ComboBox fx:id="managerComboBox" prefHeight="26.0" prefWidth="225.0"
                                                  promptText="Veuillez choisir un manager">
                                            <HBox.margin>
                                                <Insets bottom="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </ComboBox>
                                        <TextField fx:id="phoneNumberInput" promptText="Numéro de téléphone">
                                            <HBox.margin>
                                                <Insets bottom="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                                <HBox>
                                    <children>
                                        <Label text="Horaire de travail :">
                                            <HBox.margin>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <JFXTimePicker fx:id="de">
                                            <HBox.margin>
                                                <Insets bottom="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </JFXTimePicker>
                                        <JFXTimePicker fx:id="vers">
                                            <HBox.margin>
                                                <Insets bottom="20.0" right="20.0" top="20.0"/>
                                            </HBox.margin>
                                        </JFXTimePicker>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER">
                                    <children>
                                        <JFXButton fx:id="registerButton" text="Joignez-Nous"/>
                                        <JFXButton fx:id="createButton" text="Créer assocation"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
